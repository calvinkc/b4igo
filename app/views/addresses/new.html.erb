<title> B4iGo - Address Entry</title>
<h1>Address Entry!</h1>
<%= form_tag "/addresses", method: :post do %>
  <div>
    <%= label_tag :Location %>
    <%= text_field_tag :location, nil, :id => 'input' %>

    <%= label_tag :Label %>
    <%= text_field_tag :label %>

    <%= submit_tag "Submit" %>
  </div>
<% end %>
